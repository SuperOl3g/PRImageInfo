{
    "manifest_version": 2,
    "name": "PRImageInfo",
    "description": "Add information about image size, width, height to Bitbucket Pull Request pages.",
    "default_locale": "en",
    "version": "0.1.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "bundle.js"
            ]
        }
    ]
}